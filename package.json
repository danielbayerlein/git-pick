{
  "name": "@danielbayerlein/git-pick",
  "version": "1.0.3",
  "description": "git cherry-pick to multiple branches",
  "main": "lib/index.js",
  "bin": {
    "git-pick": "bin/cli.js"
  },
  "scripts": {
    "coverage": "jest --coverage",
    "lint": "eslint --format=node_modules/eslint-formatter-pretty .",
    "test": "npm run lint && jest",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielbayerlein/git-pick.git"
  },
  "keywords": [
    "Git",
    "git-cherry-pick",
    "cherry-pick"
  ],
  "author": "Daniel Bayerlein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danielbayerlein/git-pick/issues"
  },
  "homepage": "https://github.com/danielbayerlein/git-pick#readme",
  "devDependencies": {
    "eslint": "^3.14.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-import": "^2.2.0",
    "husky": "^0.12.0",
    "jest": "^18.1.0"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "log-symbols": "^1.0.2",
    "shelljs": "^0.7.6",
    "update-notifier": "^1.0.3"
  },
  "jest": {
    "bail": true,
    "collectCoverage": "true",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "**/{lib,bin}/*.js"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
